<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.solution.appsolute.dao.login.LoginDao">
    <select id="login" resultType="com.solution.appsolute.dao.login.LoginDto">
        SELECT * FROM emp WHERE empnum = #{empNum} AND emppassword = #{empPassword}
    </select> 
</mapper>
